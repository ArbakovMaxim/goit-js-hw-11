{{#each this}}
<div class="photo-card">
  <a href="{{largeImageURL}}">
  <img class="photo-card--img" src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
  </a>
  <div class="info">
    <p class="info-item">
      <b>Likes: <br><span class="info-item__list">{{likes}}</span></b>
    </p>
    <p class="info-item">
      <b>Views: <br> <span class="info-item__list">{{views}}</span></b>
    </p>
    <p class="info-item">
      <b>Comments: <br> <span class="info-item__list">{{comments}}</span></b>
    </p>
    <p class="info-item">
      <b>Downloads: <br> <span class="info-item__list">{{downloads}}</span></b>
    </p>
  </div>
</div>
{{/each}}
